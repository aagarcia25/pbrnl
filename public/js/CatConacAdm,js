
$(document).ready(function() {
    Eventos();
});

function Eventos() {
    
}

function ver_opciones(opcion) {
    $(".grupo-catalogos").addClass("d-none");
    $("#" + opcion).removeClass("d-none");
}

function img_over(id, nombre) {
    $("#" + id).attr('src', nombre);
}

function img_out(id, nombre) {
    $("#" + id).attr('src', nombre);
}
